{
  "name": "@valet/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./schemas": {
      "types": "./src/schemas/index.ts",
      "default": "./dist/schemas/index.js"
    },
    "./constants": {
      "types": "./src/constants/index.ts",
      "default": "./dist/constants/index.js"
    },
    "./errors": {
      "types": "./src/errors/index.ts",
      "default": "./dist/errors/index.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./dist/types/index.js"
    },
    "./env": {
      "types": "./src/env.ts",
      "default": "./dist/env.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run"
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
